{
  "name": "rdf-express-node-factory",
  "version": "0.0.0",
  "description": "Express middleware which sets up an RDF/JS factory creating named nodes relative to the request URL",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx yarn-deduplicate",
    "prepare": "husky install",
    "prepack": "tsc",
    "lint": "eslint . --ext .ts --quiet --ignore-path .gitignore --ignore-path .eslintignore",
    "test": "c8 --all --reporter=lcov mocha --recursive --extension ts test",
    "release": "changeset publish"
  },
  "dependencies": {
    "absolute-url": "^1.2.2",
    "is-absolute-url": "^3.0.3",
    "rdf-ext": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@changesets/cli": "^2.14.1",
    "@tpluscode/eslint-config": "^0.1.1",
    "@types/absolute-url": "^1.2.0",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@types/rdf-ext": "^1.3.8",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "c8": "^7.7.0",
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "typescript": "^4.2.3",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "standard": "^16.0.3",
    "supertest": "^6.1.3"
  },
  "mocha": {
    "watch-files": [
      "./**/*.ts"
    ],
    "require": [
      "mocha-setup.js"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  }
}
